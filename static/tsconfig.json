{
    "compilerOptions": {
        "target": "es2016",
        "module": "commonjs",
        "rootDir": "./ts",
        "outDir": "./js",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "noEmitOnError": true
    },

    "exclude": ["ts/auto.ts"]
}
